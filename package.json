    "webpack": "webpack",
    "build": "cross-env NODE_ENV=production run-s build:tsc webpack"
  },
  "dependencies": {
    "@noliaki/eslint-config-prettier": "^0.0.2",
    "@noliaki/prettier-config": "^0.0.1",
    "@types/express": "^4.16.1",
    "@types/node": "^11.13.6",
    "@types/puppeteer": "^1.12.3",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "cross-env": "^5.2.0",
    "ejs": "^2.5.6",
    "eslint": "^5.16.0",
    "express": "^4.15.2",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "puppeteer": "^1.14.0",
    "terser-webpack-plugin": "^1.2.3",
    "typescript": "^3.4.4",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "devDependencies": {
    "request": "^2.81.0",
    "tape": "^4.7.0"
  },
  "repository": {

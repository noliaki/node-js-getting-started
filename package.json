{
  "name": "node-js-getting-started",
  "version": "0.3.0",
  "description": "A sample Node.js app using Express 4",
  "engines": {
    "node": "10.x"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "test": "node test.js",
    "dev": "cross-env NODE_ENV=development run-p dev:tsc nodemon webpack",
    "dev:tsc": "tsc -w",
    "build:tsc": "tsc",
